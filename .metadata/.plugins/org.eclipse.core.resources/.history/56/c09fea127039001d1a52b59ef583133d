package 정적스택틱;
//이 클래스가 한번 불릴 때마다 1씩 증가
public class Counter {
	static int count = 0;
	public Counter(){
		count++;
		System.out.println(count);
	}
	
	public static void main(String[] args) {
		Counter c1 = new Counter();
		Counter c2 = new Counter();
		Counter c3 = new Counter();
		Counter c4 = new Counter();
	}
}
