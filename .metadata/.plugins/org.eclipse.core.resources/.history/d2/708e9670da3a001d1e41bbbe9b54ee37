
public class typeFire extends pokemon{

	public typeFire(String name,int maxHp,int attack, int defence) {
		super(name);
		super.type = "불";
		super.maxHp = maxHp;
		super.presentHp = maxHp;
		super.attack = attack;
		super.defence = defence;
    }
	public static int 몸통박치기(pokemon enemy) {
		int damage = 10;
		System.out.println("몸통박치기를 사용했다!");
		return damage - enemy.defence;
	}
	
	public static int 화염방사기(pokemon enemy) {
		int damage = 25;
		System.out.println("화염방사기 스킬을 사용했다..!!");
		return damage - enemy.defence; 
	}
	public static int 불기둥(pokemon enemy) {
		int damage = 30;
		System.out.println("불기둥 스킬을 사용했다..!!");
		return damage - enemy.defence;
		
	}

}
